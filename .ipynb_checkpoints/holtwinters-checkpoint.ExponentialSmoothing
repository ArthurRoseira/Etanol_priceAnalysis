   "source": [
    "index = df.index\n",
    "df = df.merge(\n",
    "    infl.loc[:, ['Ano', 'Mês', 'Acumulado']], how='left', on=['Ano', 'Mês'])\n",
    "df['Preço Ajustado'] = (\n",
    "    df['Preço R$']/df['Acumulado']) * inf_actual\n",
    "df.set_index(index, inplace=True)\n",
    "df.dropna(inplace=True)\n",
    "df.head()"
   ]
